package flower.utils
{
	/**
	 * 计算公式，可以随意扩展
	 */
	dynamic public class Formula
	{	
		public static function add(a:Number,b:Number):Number {
			return a + b;
		}
		
		public static function minus(a:Number,b:Number):Number {
			return a - b;
		}
		
		public static function multiply(a:Number,b:Number):Number {
			return a * b;
		}
		
		public static function device(a:Number,b:Number):Number {
			return a / b;
		}
	}
}