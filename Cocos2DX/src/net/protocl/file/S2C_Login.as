package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S2C_Login extends MessageBase implements IMessage	{		public var result:int = 0;		public var account_id:int = 0;		public var domain:String = "";		public var port:int = 0;		public var sys_open_id:String = "";		public function S2C_Login()		{			super(ProtoclId.S2C_Login);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeShort(bytes,result);			encodeInt32(bytes,account_id);			encodeStringLength(bytes,domain,32);			encodeShort(bytes,port);			encodeStringLength(bytes,sys_open_id,64);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			result = decodeShort(bytes);			account_id = decodeInt32(bytes);			domain = decodeStringLength(bytes,32);			port = decodeShort(bytes);			sys_open_id = decodeStringLength(bytes,64);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "result:" + result);			func(p + "account_id:" + account_id);			func(p + "domain:" + domain);			func(p + "port:" + port);			func(p + "sys_open_id:" + sys_open_id);			p = p.slice(1,p.length);			func(p + "}");		}	}}