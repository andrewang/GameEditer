package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S2C_PlayerWalk extends MessageBase implements IMessage	{		public var role_id:int = 0;		public var pos_x:int = 0;		public var pos_y:int = 0;		public function S2C_PlayerWalk()		{			super(ProtoclId.S2C_PlayerWalk);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeInt32(bytes,role_id);			encodeInt32(bytes,pos_x);			encodeInt32(bytes,pos_y);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			role_id = decodeInt32(bytes);			pos_x = decodeInt32(bytes);			pos_y = decodeInt32(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "role_id:" + role_id);			func(p + "pos_x:" + pos_x);			func(p + "pos_y:" + pos_y);			p = p.slice(1,p.length);			func(p + "}");		}	}}