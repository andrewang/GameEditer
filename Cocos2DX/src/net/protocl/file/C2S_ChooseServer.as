package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class C2S_ChooseServer extends MessageBase implements IMessage	{		public var server_id:int = 0;		public function C2S_ChooseServer()		{			super(ProtoclId.C2S_ChooseServer);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeInt32(bytes,server_id);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			server_id = decodeInt32(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "server_id:" + server_id);			p = p.slice(1,p.length);			func(p + "}");		}	}}