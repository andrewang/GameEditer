package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S_2_C_1V1_ROLE_INFO_SKILL extends MessageBase implements IMessage	{		public var id:int = 0;		public var level:int = 0;		public function S_2_C_1V1_ROLE_INFO_SKILL()		{			super(ProtoclId.S_2_C_1V1_ROLE_INFO_SKILL);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeInt32(bytes,id);			encodeInt32(bytes,level);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			id = decodeInt32(bytes);			level = decodeInt32(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "id:" + id);			func(p + "level:" + level);			p = p.slice(1,p.length);			func(p + "}");		}	}}