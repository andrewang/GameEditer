package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S_2_C_1V1_MATCHED extends MessageBase implements IMessage	{		public var role_id:int = 0;	// 对手的角色 ID，负数表示机器人		public var index:int = 0;	// 地图内玩家编号，用于标识出生点等		public function S_2_C_1V1_MATCHED()		{			super(ProtoclId.S_2_C_1V1_MATCHED);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeInt32(bytes,role_id);			encodeByte(bytes,index);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			role_id = decodeInt32(bytes);			index = decodeByte(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "role_id:" + role_id);			func(p + "index:" + index);			p = p.slice(1,p.length);			func(p + "}");		}	}}