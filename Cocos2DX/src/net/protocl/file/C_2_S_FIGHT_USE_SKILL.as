package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class C_2_S_FIGHT_USE_SKILL extends MessageBase implements IMessage	{		public var time:Number = 0;		public var x:int = 0;		public var y:int = 0;		public var skillId:int = 0;		public function C_2_S_FIGHT_USE_SKILL()		{			super(ProtoclId.C_2_S_FIGHT_USE_SKILL);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeInt64(bytes,time);			encodeShort(bytes,x);			encodeShort(bytes,y);			encodeInt32(bytes,skillId);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			time = decodeInt64(bytes);			x = decodeShort(bytes);			y = decodeShort(bytes);			skillId = decodeInt32(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "time:" + time);			func(p + "x:" + x);			func(p + "y:" + y);			func(p + "skillId:" + skillId);			p = p.slice(1,p.length);			func(p + "}");		}	}}