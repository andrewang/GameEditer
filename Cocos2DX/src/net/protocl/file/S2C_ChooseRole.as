package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S2C_ChooseRole extends MessageBase implements IMessage	{		public var role_name:String = "";		public var level:int = 0;		public var exp:int = 0;		public var scene_id:int = 0;		public var profession:int = 0;		public var gold:int = 0;		public var money:int = 0;		public var vip_level:int = 0;		public var mp:int = 0;		public var hp:int = 0;		public var rmb:int = 0;		public function S2C_ChooseRole()		{			super(ProtoclId.S2C_ChooseRole);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeStringLength(bytes,role_name,32);			encodeShort(bytes,level);			encodeInt32(bytes,exp);			encodeInt32(bytes,scene_id);			encodeByte(bytes,profession);			encodeInt32(bytes,gold);			encodeInt32(bytes,money);			encodeInt32(bytes,vip_level);			encodeInt32(bytes,mp);			encodeInt32(bytes,hp);			encodeInt32(bytes,rmb);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			role_name = decodeStringLength(bytes,32);			level = decodeShort(bytes);			exp = decodeInt32(bytes);			scene_id = decodeInt32(bytes);			profession = decodeByte(bytes);			gold = decodeInt32(bytes);			money = decodeInt32(bytes);			vip_level = decodeInt32(bytes);			mp = decodeInt32(bytes);			hp = decodeInt32(bytes);			rmb = decodeInt32(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "role_name:" + role_name);			func(p + "level:" + level);			func(p + "exp:" + exp);			func(p + "scene_id:" + scene_id);			func(p + "profession:" + profession);			func(p + "gold:" + gold);			func(p + "money:" + money);			func(p + "vip_level:" + vip_level);			func(p + "mp:" + mp);			func(p + "hp:" + hp);			func(p + "rmb:" + rmb);			p = p.slice(1,p.length);			func(p + "}");		}	}}