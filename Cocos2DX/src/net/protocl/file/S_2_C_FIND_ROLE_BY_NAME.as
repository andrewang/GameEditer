package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	//查找角色	public class S_2_C_FIND_ROLE_BY_NAME extends MessageBase implements IMessage	{		public var role_id:int = 0;		public var level:int = 0;		public var profession:int = 0;		public var vip_level:int = 0;		public function S_2_C_FIND_ROLE_BY_NAME()		{			super(ProtoclId.S_2_C_FIND_ROLE_BY_NAME);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeInt32(bytes,role_id);			encodeShort(bytes,level);			encodeByte(bytes,profession);			encodeInt32(bytes,vip_level);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			role_id = decodeInt32(bytes);			level = decodeShort(bytes);			profession = decodeByte(bytes);			vip_level = decodeInt32(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "role_id:" + role_id);			func(p + "level:" + level);			func(p + "profession:" + profession);			func(p + "vip_level:" + vip_level);			p = p.slice(1,p.length);			func(p + "}");		}	}}