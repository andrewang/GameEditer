package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S_2_C_FIGHT_HURT extends MessageBase implements IMessage	{		public var role_id:int = 0;		public var time:Number = 0;		public var type:int = 0;	//受伤类型		public var hp:int = 0;	//受伤血量		public function S_2_C_FIGHT_HURT()		{			super(ProtoclId.S_2_C_FIGHT_HURT);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeInt32(bytes,role_id);			encodeInt64(bytes,time);			encodeByte(bytes,type);			encodeInt32(bytes,hp);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			role_id = decodeInt32(bytes);			time = decodeInt64(bytes);			type = decodeByte(bytes);			hp = decodeInt32(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "role_id:" + role_id);			func(p + "time:" + time);			func(p + "type:" + type);			func(p + "hp:" + hp);			p = p.slice(1,p.length);			func(p + "}");		}	}}