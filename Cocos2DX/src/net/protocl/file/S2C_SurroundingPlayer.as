package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S2C_SurroundingPlayer extends MessageBase implements IMessage	{		public var players:Vector.<ClosePlayerInfo> = new Vector.<ClosePlayerInfo>;		public function S2C_SurroundingPlayer()		{			super(ProtoclId.S2C_SurroundingPlayer);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeRepeatMessage(bytes,players);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			decodeRepeatMessage(bytes,ClosePlayerInfo,players);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "[");			for(var i:int = 0; i < players.length; i++)			{				this.players[i].getString(func,p + "	");			}			func(p + "]");			p = p.slice(1,p.length);			func(p + "}");		}	}}