package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class C2S_EnterInstanceScene extends MessageBase implements IMessage	{		public var mapId:int = 0;		public var mode:int = 0;		public function C2S_EnterInstanceScene()		{			super(ProtoclId.C2S_EnterInstanceScene);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeInt32(bytes,mapId);			encodeByte(bytes,mode);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			mapId = decodeInt32(bytes);			mode = decodeByte(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "mapId:" + mapId);			func(p + "mode:" + mode);			p = p.slice(1,p.length);			func(p + "}");		}	}}