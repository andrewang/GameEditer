package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S2C_LoadServerTowns extends MessageBase implements IMessage	{		public var towns:Vector.<TownInfo> = new Vector.<TownInfo>;		public function S2C_LoadServerTowns()		{			super(ProtoclId.S2C_LoadServerTowns);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeRepeatMessage(bytes,towns);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			decodeRepeatMessage(bytes,TownInfo,towns);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "[");			for(var i:int = 0; i < towns.length; i++)			{				this.towns[i].getString(func,p + "	");			}			func(p + "]");			p = p.slice(1,p.length);			func(p + "}");		}	}}