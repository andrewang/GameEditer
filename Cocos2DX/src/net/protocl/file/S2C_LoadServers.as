package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S2C_LoadServers extends MessageBase implements IMessage	{		public var servers:Vector.<ServerInfo> = new Vector.<ServerInfo>;		public function S2C_LoadServers()		{			super(ProtoclId.S2C_LoadServers);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeRepeatMessage(bytes,servers);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			decodeRepeatMessage(bytes,ServerInfo,servers);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "[");			for(var i:int = 0; i < servers.length; i++)			{				this.servers[i].getString(func,p + "	");			}			func(p + "]");			p = p.slice(1,p.length);			func(p + "}");		}	}}