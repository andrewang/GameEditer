package net.protocl.file{	import net.protocl.MyByteArray;	import net.protocl.MessageBase;	import net.protocl.IMessage;	public class S_2_C_LOAD_SIGN_IN_INFO extends MessageBase implements IMessage	{		public var cycle:int = 0;		public var day:int = 0;		public var rewarded:int = 0;		public function S_2_C_LOAD_SIGN_IN_INFO()		{			super(ProtoclId.S_2_C_LOAD_SIGN_IN_INFO);		}		//打包函数		public function encode():MyByteArray		{			var bytes:MyByteArray = new MyByteArray();			encodeByte(bytes,cycle);			encodeByte(bytes,day);			encodeByte(bytes,rewarded);			return bytes;		}		//解包函数		public function decode(bytes:MyByteArray):void		{			cycle = decodeByte(bytes);			day = decodeByte(bytes);			rewarded = decodeByte(bytes);		}		//打印函数		override public function getString(func,p:String="")		{			func(p + "{");			p += "	";			func(p + "cycle:" + cycle);			func(p + "day:" + day);			func(p + "rewarded:" + rewarded);			p = p.slice(1,p.length);			func(p + "}");		}	}}